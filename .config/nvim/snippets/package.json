{
    "name": "plovinicius-custom-snippets",
    "engine": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": "php",
                "path": "./php.json"
            },
            {
                "language": "rust",
                "path": "./rust.json"
            },
            {
                "language": "typescript",
                "path": "./typescript.json"
            },
            {
                "language": "all",
                "path": "./global.json"
            }
        ]
    }
}
